
<h1>Create your doggy profile below!</h1>

<%= form_for @dog do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>
   <%= f.label :email %>
   <%= f.text_field :email %>
   <%= f.label :password %>
   <%= f.password_field :password %>
   <%= f.label "Confirm Password" %>
   <%= f.password_field :password_confirmation %>
   <%= f.submit "Sign Up"  %>
<% end %>