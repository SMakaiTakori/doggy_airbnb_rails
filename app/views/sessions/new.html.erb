<% if @session.errors.any? %>
  <ul>
    <% @session.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <%end%>
  </ul>
<%end%>

<h1>Login Below!</h1>

<%= form_for :dog do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %><br>

   <%= f.label :email %>
   <%= f.email_field :email %><br>

   <%= f.label :password %>
   <%= f.password_field :password %><br>
   
   <%= f.submit "Log In"  %>
<% end %>